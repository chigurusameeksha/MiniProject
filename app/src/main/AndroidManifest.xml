<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.miniproject.healthcaree">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.miniproject.healthcaree.Acne"></activity>
        <activity android:name="com.miniproject.healthcaree.Pregnancy" />
        <activity android:name="com.miniproject.healthcaree.Periods" />
        <activity android:name="com.miniproject.healthcaree.Migrane" />
        <activity android:name="com.miniproject.healthcaree.Fever" />
        <activity android:name="com.miniproject.healthcaree.Cough" />
        <activity android:name="com.miniproject.healthcaree.main" />
        <activity android:name="com.miniproject.healthcaree.Home" />
        <activity android:name="com.miniproject.healthcaree.Services" />
        <activity android:name="com.miniproject.healthcaree.HealthTips" />
        <activity android:name="com.miniproject.healthcaree.KnowYourMed" />
        <activity android:name="com.miniproject.healthcaree.AyurvedicTips" />
        <activity android:name="com.miniproject.healthcaree.FirstAidTips" />
        <activity android:name="com.miniproject.healthcaree.HealthSchemes" />
        <activity android:name="com.miniproject.healthcaree.Cold" />
        <activity android:name="com.miniproject.healthcaree.StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.miniproject.healthcaree.SpecialSearch" />
        <activity android:name="com.miniproject.healthcaree.SpecialRecords" />
    </application>

</manifest>