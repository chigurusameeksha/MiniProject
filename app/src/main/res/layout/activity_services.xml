<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/gradient_background">

    <ImageButton
        android:id="@+id/Specialization"
        android:layout_width="158dp"
        android:layout_height="164dp"
        android:layout_above="@+id/imageButton18"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.159"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.827"
        app:srcCompat="@drawable/ic_people"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="157dp"
        android:layout_height="89dp"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignStart="@+id/Specialization"

        android:layout_alignLeft="@+id/Specialization"
        android:layout_alignBottom="@+id/textView6"
        android:layout_marginStart="23dp"
        android:text="Specialized \nHospitals"
        android:textColor="#000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.787" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="132dp"
        android:layout_height="59dp"

        android:layout_alignEnd="@+id/Hospital"
        android:layout_alignRight="@+id/Hospital"
        android:layout_alignBottom="@+id/Hospital"
        android:layout_marginBottom="8dp"
        android:text=" Near by Hospitals"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="#000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.364" />

    <ImageButton
        android:id="@+id/Hospital"
        android:layout_width="158dp"
        android:layout_height="171dp"
        android:layout_alignTop="@+id/Specialization"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.351"
        app:srcCompat="@drawable/ic_001_medical"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginStart="71dp"
        android:text="Current Location"
        android:textColor="#000"
        android:textSize="22dp"
        android:textStyle="bold"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.263"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/NullState"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Statename"
        android:layout_alignBottom="@+id/Statename"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:text="State :"
        android:textColor="#000"
        android:textSize="20dp"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.149"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.085" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Cityname"
        android:layout_alignStart="@+id/Specialization"
        android:layout_alignLeft="@+id/Specialization"
        android:layout_alignBottom="@+id/Cityname"
        android:text="City :"
        android:textColor="#000"
        android:textSize="20dp"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182" />

    <TextView
        android:id="@+id/Statename"
        android:layout_width="219dp"
        android:layout_height="59dp"
        android:layout_alignTop="@+id/textView8"
        android:layout_toStartOf="@+id/textView8"
        android:layout_toLeftOf="@+id/textView8"
        android:textColor="#000"
        android:textSize="18dp"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.088" />

    <TextView
        android:id="@+id/Cityname"
        android:layout_width="213dp"
        android:layout_height="61dp"
        android:layout_above="@+id/Specialization"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:textColor="#000"
        android:textSize="17dp"
        app:fontFamily="@font/playfair_display_italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.191" />

</android.support.constraint.ConstraintLayout>
